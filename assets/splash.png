<svg width="1284" height="2778" viewBox="0 0 1284 2778" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B35;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FF8C42;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFA366;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1284" height="2778" fill="url(#bgGradient)"/>
  
  <!-- Main logo container -->
  <g transform="translate(142, 1000)">
    <!-- Logo background circle -->
    <circle cx="500" cy="400" r="350" fill="#fff" fill-opacity="0.9"/>
    
    <!-- Delivery truck icon -->
    <g transform="translate(200, 200)">
      <!-- Truck body -->
      <rect x="0" y="100" width="300" height="150" rx="20" fill="#FF6B35"/>
      <rect x="300" y="120" width="120" height="130" rx="15" fill="#FF6B35"/>
      
      <!-- Truck cabin -->
      <rect x="320" y="80" width="80" height="80" rx="10" fill="#FF8C42"/>
      
      <!-- Wheels -->
      <circle cx="80" cy="280" r="35" fill="#333"/>
      <circle cx="80" cy="280" r="20" fill="#666"/>
      <circle cx="350" cy="280" r="35" fill="#333"/>
      <circle cx="350" cy="280" r="20" fill="#666"/>
      
      <!-- Speed lines -->
      <g stroke="#fff" stroke-width="8" stroke-linecap="round">
        <line x1="450" y1="120" x2="500" y2="120"/>
        <line x1="460" y1="150" x2="520" y2="150"/>
        <line x1="450" y1="180" x2="510" y2="180"/>
      </g>
      
      <!-- Package icon -->
      <rect x="120" y="130" width="60" height="60" rx="5" fill="#fff" stroke="#FF6B35" stroke-width="3"/>
      <line x1="120" y1="160" x2="180" y2="160" stroke="#FF6B35" stroke-width="3"/>
      <line x1="150" y1="130" x2="150" y2="190" stroke="#FF6B35" stroke-width="3"/>
    </g>
  </g>
  
  <!-- App name -->
  <text x="642" y="1600" font-family="Arial, sans-serif" font-size="120" font-weight="bold" text-anchor="middle" fill="#fff">RAPIFLOW</text>
  
  <!-- Tagline -->
  <text x="642" y="1700" font-family="Arial, sans-serif" font-size="40" text-anchor="middle" fill="#fff" fill-opacity="0.9">Entregas rápidas en Mayarí Arriba</text>
  
  <!-- Loading indicator -->
  <g transform="translate(592, 2200)">
    <circle cx="50" cy="50" r="40" fill="none" stroke="#fff" stroke-width="6" stroke-linecap="round" stroke-dasharray="60 40">
      <animateTransform attributeName="transform" type="rotate" values="0 50 50;360 50 50" dur="1s" repeatCount="indefinite"/>
    </circle>
  </g>
</svg>