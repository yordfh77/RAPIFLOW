<svg width="1024" height="1024" viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8C42;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Delivery truck icon centered -->
  <g transform="translate(150, 250)">
    <!-- Truck body -->
    <rect x="0" y="100" width="400" height="200" rx="25" fill="#fff"/>
    <rect x="400" y="130" width="160" height="170" rx="20" fill="#fff"/>
    
    <!-- Truck cabin -->
    <rect x="430" y="80" width="100" height="100" rx="15" fill="#e0e0e0"/>
    
    <!-- Wheels -->
    <circle cx="100" cy="340" r="45" fill="#333"/>
    <circle cx="100" cy="340" r="25" fill="#666"/>
    <circle cx="460" cy="340" r="45" fill="#333"/>
    <circle cx="460" cy="340" r="25" fill="#666"/>
    
    <!-- Speed lines -->
    <g stroke="url(#gradient)" stroke-width="10" stroke-linecap="round">
      <line x1="580" y1="150" x2="650" y2="150"/>
      <line x1="590" y1="190" x2="670" y2="190"/>
      <line x1="580" y1="230" x2="660" y2="230"/>
    </g>
    
    <!-- Package icon -->
    <rect x="160" y="150" width="80" height="80" rx="8" fill="#FF6B35" stroke="#fff" stroke-width="4"/>
    <line x1="160" y1="190" x2="240" y2="190" stroke="#fff" stroke-width="4"/>
    <line x1="200" y1="150" x2="200" y2="230" stroke="#fff" stroke-width="4"/>
  </g>
</svg>