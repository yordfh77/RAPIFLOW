<svg width="1024" height="1024" viewBox="0 0 1024 1024" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B35;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8C42;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="512" cy="512" r="480" fill="url(#gradient)" stroke="#fff" stroke-width="20"/>
  
  <!-- Delivery truck icon -->
  <g transform="translate(200, 300)">
    <!-- Truck body -->
    <rect x="0" y="100" width="300" height="150" rx="20" fill="#fff"/>
    <rect x="300" y="120" width="120" height="130" rx="15" fill="#fff"/>
    
    <!-- Truck cabin -->
    <rect x="320" y="80" width="80" height="80" rx="10" fill="#e0e0e0"/>
    
    <!-- Wheels -->
    <circle cx="80" cy="280" r="35" fill="#333"/>
    <circle cx="80" cy="280" r="20" fill="#666"/>
    <circle cx="350" cy="280" r="35" fill="#333"/>
    <circle cx="350" cy="280" r="20" fill="#666"/>
    
    <!-- Speed lines -->
    <g stroke="#fff" stroke-width="8" stroke-linecap="round">
      <line x1="450" y1="120" x2="500" y2="120"/>
      <line x1="460" y1="150" x2="520" y2="150"/>
      <line x1="450" y1="180" x2="510" y2="180"/>
    </g>
    
    <!-- Package icon -->
    <rect x="120" y="130" width="60" height="60" rx="5" fill="#FF6B35" stroke="#fff" stroke-width="3"/>
    <line x1="120" y1="160" x2="180" y2="160" stroke="#fff" stroke-width="3"/>
    <line x1="150" y1="130" x2="150" y2="190" stroke="#fff" stroke-width="3"/>
  </g>
  
  <!-- App name -->
  <text x="512" y="750" font-family="Arial, sans-serif" font-size="80" font-weight="bold" text-anchor="middle" fill="#fff">RAPIFLOW</text>
</svg>